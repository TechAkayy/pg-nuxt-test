{
  "name": "pg-nuxt",
  "version": "0.0.0",
  "homepage": "https://pg-nuxt-css.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-nuxt.git"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "now": "npm run build && npm run preview",
    "postinstall": "nuxt prepare",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write . '!public'"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.4.5",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinegrow/nuxt-module": "^3.0.0-beta.134",
    "@vue/devtools": "^6.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "npm-run-all": "^4.1.5",
    "@types/node": "^18",
    "nuxt": "^3.5.2",
    "nuxt-icon": "^0.4.0",
    "prettier": "^2.8.4",
    "typescript": "^5.0.4"
  },
  "web-types": "web-types.json"
}

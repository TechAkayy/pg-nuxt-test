{
  "$schema": "http://json.schemastore.org/web-types",
  "name": "project",
  "framework": "vue",
  "version": "0.0.0",
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "BaseButton",
          "attributes": [
            {
              "name": "size",
              "customType": "select",
              "default": "sm",
              "options": ["2xs", "xs", "sm", "md", "lg", "xl"]
            },
            {
              "name": "variant",
              "customType": "select",
              "default": "solid",
              "options": ["solid", "outline", "soft", "ghost", "link"]
            },
            {
              "name": "target",
              "customType": "select",
              "default": "",
              "options": ["_blank"]
            }
          ]
        }
      ],
      "attributes": [
        {
          "name": "color",
          "customType": "color",
          "options": []
        }
      ]
    }
  }
}
